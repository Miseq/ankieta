<h1>{{ form['#title'] }}</h1>

<div class="layout-row clearfix">
  <div class="layout-column layout-column--half">
    <div class="">
      <div class="">
        <p>{{ form['#table'] }}</p>
        <p>{{ form['#top_info'] }}</p>
        <p>{{ form['#date_created'] }}</p>
        <p>
          {{ form['#last_info'] }}        
          {{ form.gen_token }}
          {{ form.remove_token }}
        </p>
        <p>{{ form['#date_gen'] }}</p>
      </div>
    </div>
    <div class="">
      <div class="">
        {{ form.number_begin }}
        {{ form.number_end }}
        {{ form.key }}
      </div>
      <div class="">
        {{ form.select_questionnaire }}
        {{ form.select_questionnaire_back }}
      </div>
        {{ form.confirm_remove }}
        {{ form.cancel_remove }}
    </div>
  </div>
</div>